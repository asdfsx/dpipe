{
    // global conf shared by all projects

    projects: [
        {
            name:       "rs"
        }
    ]

    plugins: [
        {
            name:   "AlsLogInput"
            class:  "AlsLogInput"
            ticker_interval: 30
            sources: [
                {
                    glob:   "/var/log/a*.log"
                    project:   "rs"
                    tail: false
                    ident:   "alog"
                }
            ]
        }
        
        {
            name:   "DebugOutput"
            blackhole: false
            match:  ["alog", ]
        }
    ]

}
