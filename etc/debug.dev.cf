{
    projects: [
        {
            name: "mac"
        }
    ]

    plugins: [
        {
            name:   "AlsLogInput"
            ticker_interval: 30
            projects: [
                name: "mac"
                sources: [
                    {
                        glob: "/var/log/a*.log"
                        ident: "alog"
                        tail: false
                    }
                ]

            ]
        }
        
        {
            name:   "DebugOutput"
            blackhole: false
            match:  ["alog", ]
        }
    ]

}
